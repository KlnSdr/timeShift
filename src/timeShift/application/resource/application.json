{
  "dobby": {
    "port": 3456,
    "threads": 10,
    "staticContent": {
      "directory": "static"
    },
    "session": {
      "maxAge": 1,
      "store": "hades.session.HadesSessionStore"
    },
    "logLevel": "DEBUG"
  },
  "application": {
    "name": "timeShift",
    "version": "1.4",
    "devMode": false,
    "dbHost": "thot-container"
  },
  "hades": {
    "login": {
      "maxAttempts": 5,
      "lockDuration": 300000,
      "redirect": {
        "success": "/",
        "successAdmin": "/hades"
      }
    },
    "unauthorizedRedirectTarget": "/hades/login",
    "unauthorizedRedirectPaths": [
      "/",
      "/index.html"
    ]
  }
}